@model IEnumerable<KF31_WebApp.Models.Employ>

@{
    ViewData["Title"] = "Employ";
}

<h1>商品一覧</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.EmployID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EmployName)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.EmployID)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.EmployName)
                </td>

                @* <td>
            <a asp-action="ItemDetail" asp-route-id="@item.Code">
            <img src="~/pictures/@item.Image">
            </a>
            </td> *@
            </tr>
        }
    </tbody>
</table>
