@model IEnumerable<KF31_WebApp.Models.Employ>

@{
    ViewData["Title"] = "Employ";
}

<h1>商品一覧</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.EmployID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Em_DisplayName)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.EmployID)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Em_DisplayName)
                </td>
               
            </tr>
        }
    </tbody>
    
    


</table>
